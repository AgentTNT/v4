{# app/Resources/views/ipv4address/index.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}{% trans %}Welcome to{% endtrans %} eFa -- {% trans %}IPv4 Address{% endtrans %}{% endblock %}
{% block body %}
    <div class="initbody">
        <div class="init">
            <div class="center">
                    <img src="{{ asset('images/logo.png') }}" alt="eFa Logo" />
            </div>
            <h1>{% trans %}IPv4 Address{% endtrans %}</h1>
           <div class="inner-container">
                {{ form_start(form) }}
                <h2>
                    {{ form_label(form.IPv4address) }}
                </h2>
                <div>
                    {{ form_widget(form.IPv4address) }}
                </div>
                {{ form_widget(form.Back) }} 
                {{ form_widget(form.Next) }}
            </div>
            {{ form_end(form) }}
            <div class="error-container">
                {{ form_errors(form) }}
            </div>
        </div>
    </div>
{% endblock %}
