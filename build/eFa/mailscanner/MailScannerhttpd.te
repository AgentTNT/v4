
module MailScannerhttpd 1.0;

require {
    type httpd_t;
    type mscan_etc_t;
    class file { getattr open };
    class file { getattr read };
    class dir { getattr read };
}

#============= httpd_t ==============
allow httpd_t mscan_etc_t:file { getattr open };
allow httpd_t mscan_etc_t:file { getattr read };
allow httpd_t mscan_etc_t:dir { getattr read };
